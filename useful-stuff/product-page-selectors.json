{
  "selectors": [
    {
      "field": "Name",
      "selector": ".pip h3",
      "multiple": false
    },
    {
      "field": "Code",
      "selector": ".pip .pir ul li:contains(Code: ) span",
      "multiple": false
    },
    {
      "field": "Size",
      "selector": ".pip .pir ul li:contains(Size: ) span",
      "multiple": false
    },
    {
      "field": "WSP",
      "selector": ".pip .pir ul li:contains(WSP: ) span",
      "multiple": false
    },
    {
      "field": "WSP inc VAT",
      "selector": ".pip .pir ul li:contains(WSP inc VAT: ) span",
      "multiple": false
    },
    {
      "field": "RRP",
      "selector": ".pip .pir ul li:contains(RRP: ) span",
      "multiple": false
    },
    {
      "field": "POR",
      "selector": ".pip .pir ul li:contains(POR: ) span",
      "multiple": false
    },
    {
      "field": "VAT",
      "selector": ".pip .pir ul li:contains(VAT: ) span",
      "multiple": false
    },
    {
      "field": "Alcohol by Volume",
      "selector": ".pip .pir ul li:contains(Alcohol by Volume: ) span",
      "multiple": false
    },
    {
      "field": "Case of",
      "selector": "a[href*=\"Case\"]",
      "multiple": false
    },
    {
      "field": "Unit Description",
      "selector": ".pip .pir ul li:contains(Unit Description: ) span",
      "multiple": false
    },
    {
      "field": "On Offer",
      "selector": "a[href*=\"On+Offer\"]",
      "multiple": false
    },
    {
      "field": "picture-popup",
      "type": "SelectorPopupLink",
      "selector": ".piTopInfo div div div a",
      "multiple": false,
      "delay": 0
    },
    {
      "field": "image",
      "type": "SelectorImage",
      "parentSelectors": ["picture-popup"],
      "selector": "div img",
      "multiple": false,
      "delay": 0
    },
    {
      "field": "close-popup",
      "type": "SelectorElementClick",
      "selector": "input#btnCloseWindow",
      "multiple": false,
      "clickElementSelector": "input#btnCloseWindow",
      "clickType": "clickOnce"
    },
    {
      "field": "By Country of Origin",
      "selector": "a[href*=\"By+Country+of+Origin\"]",
      "multiple": false
    },
    {
      "field": "By Pack Type",
      "selector": "a[href*=\"By+Pack+Type\"]",
      "multiple": false
    },
    {
      "field": "By Brand",
      "selector": ".pip #catLinks b:contains(By Brand:) + span a",
      "multiple": true
    },
    {
      "field": "By Beverage Type",
      "selector": "a[href*=\"By+Beverage+Type\"]",
      "multiple": true
    },
    {
      "field": "By Additives",
      "selector": "a[href*=\"By+Additives\"]",
      "multiple": true
    },
    {
      "field": "By Storage Type",
      "selector": "a[href*=\"By+Storage+Type\"]",
      "multiple": true
    },
    {
      "field": "By Packed In",
      "selector": "a[href*=\"By+Packed+In\"]",
      "multiple": true
    },
    {
      "field": "By Current Vintage",
      "selector": "a[href*=\"By+Current+Vintage\"]",
      "multiple": true
    },
    {
      "field": "By Wine Colour",
      "selector": "a[href*=\"By+Wine+Colour\"]",
      "multiple": true
    },
    {
      "field": "By Producer",
      "selector": "a[href*=\"By+Producer\"]",
      "multiple": true
    },
    {
      "field": "By Grape Variety",
      "selector": "a[href*=\"By+Grape+Variety\"]",
      "multiple": true
    },
    {
      "field": "By Type of Closure",
      "selector": "a[href*=\"By+Type+of+Closure\"]",
      "multiple": true
    },
    {
      "field": "By Wine Maker",
      "selector": "a[href*=\"By+Wine+Maker\"]",
      "multiple": true
    },
    {
      "field": "By Product Category ",
      "selector": "a[href*=\"Product\"]",
      "multiple": true
    },
    {
      "field": "Alternative Products",
      "type": "SelectorElementAttribute",
      "selector": "h1:contains(\"Alternative Products\") + span + ul li a",
      "multiple": true,
      "extractAttribute": "href",
      "delay": 0
    },
    {
      "field": "Description",
      "selector": "h1:contains(\"Description\") + p",
      "multiple": false
    },
    {
      "field": "Tasting Notes",
      "selector": "h1:contains(\"Tasting Notes\") + p",
      "multiple": false
    },
    {
      "field": "Ingredients",
      "selector": "h1:contains(\"Ingredients\") + ul li",
      "multiple": true
    },
    {
      "field": "Allergy Advice",
      "selector": "h1:contains(\"Allergy Advice\") + ul li",
      "multiple": true
    },
    {
      "field": "Nutrition Table",
      "type": "SelectorTable",
      "selector": "h1:contains(\"Nutrition\") + div table",
      "multiple": true,
      "columns": [],
      "tableDataRowSelector": "tbody tr td",
      "tableHeaderRowSelector": "tbody tr th"
    },
    {
      "field": "Freezing Guidelines",
      "selector": "h1:contains(\"Freezing Guidelines\") + ul li",
      "multiple": true
    },
    {
      "field": "Prepare and Use",
      "selector": "h1:contains(\"Prepare and Use\") + p",
      "multiple": false
    },
    {
      "field": "Storage Information",
      "selector": "h1:contains(\"Storage Information\") + ul li",
      "multiple": true
    },
    {
      "field": "Manufacturer",
      "selector": "h1:contains(\"Manufacturer\") + p",
      "multiple": false
    },
    {
      "field": "Country",
      "selector": "h1:contains(\"Country\") + ul li",
      "multiple": true
    },
    {
      "field": "Packaging",
      "selector": "h1:contains(\"Packaging\") + ul li",
      "multiple": true
    },
    {
      "field": "Recycling",
      "selector": "h1:contains(\"Recycling\") + ul li",
      "multiple": true
    },
    {
      "field": "Additional Information",
      "selector": "h1:contains(\"Additional Information\") + p",
      "multiple": false
    },
    {
      "field": "Nutrition Notes",
      "selector": "h1:contains(\"Nutrition\") + div + p",
      "multiple": false
    }
  ]
}
