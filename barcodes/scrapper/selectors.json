{
  "_id": "booker-barcodes",
  "startUrl": ["http://127.0.0.1:8081/index.html"],
  "selectors": [
    {
      "id": "categories",
      "type": "SelectorLink",
      "parentSelectors": ["_root"],
      "selector": "a",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "sub-categories",
      "type": "SelectorLink",
      "parentSelectors": ["categories"],
      "selector": "a",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "print-product-list",
      "type": "SelectorLink",
      "parentSelectors": ["sub-categories"],
      "selector": "#printRow a",
      "multiple": false,
      "delay": 0
    },
    {
      "id": "ungrouped-proceed",
      "type": "SelectorLink",
      "parentSelectors": ["print-product-list"],
      "selector": "a#hypUngrouped2",
      "multiple": false,
      "delay": 0
    },
    {
      "id": "table-row",
      "type": "SelectorElement",
      "parentSelectors": ["ungrouped-proceed"],
      "selector": "tbody tr",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "code",
      "type": "SelectorText",
      "parentSelectors": ["table-row"],
      "selector": "td:nth-of-type(2)",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "barcode",
      "type": "SelectorElementAttribute",
      "parentSelectors": ["table-row"],
      "selector": "td img",
      "multiple": false,
      "extractAttribute": "alt",
      "delay": 0
    }
  ]
}
